{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "7c984583-40f2-490d-952c-2306175bb847"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "protec": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/protec",
      "sourceRoot": "projects/protec/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/protec",
            "index": "projects/protec/src/index.html",
            "main": "projects/protec/src/main.ts",
            "polyfills": "projects/protec/src/polyfills.ts",
            "tsConfig": "projects/protec/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/protec/src/favicon.ico",
              "projects/protec/src/assets"
            ],
            "styles": [
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
              "node_modules/primeng/resources/themes/omega/theme.scss",
              "node_modules/primeng/resources/primeng.min.css",
              "./node_modules/bootstrap/dist/css/bootstrap.min.css",
              "projects/protec/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4mb",
                  "maximumError": "4mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/protec/src/environments/environment.ts",
                  "with": "projects/protec/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "protec:build:production"
            },
            "development": {
              "browserTarget": "protec:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "protec:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/protec/src/test.ts",
            "polyfills": "projects/protec/src/polyfills.ts",
            "tsConfig": "projects/protec/tsconfig.spec.json",
            "karmaConfig": "projects/protec/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/protec/src/favicon.ico",
              "projects/protec/src/assets"
            ],
            "styles": [
              "projects/protec/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "protec-library": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/protec-library",
      "sourceRoot": "projects/protec-library/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/protec-library",
            "index": "projects/protec-library/src/index.html",
            "main": "projects/protec-library/src/main.ts",
            "polyfills": "projects/protec-library/src/polyfills.ts",
            "tsConfig": "projects/protec-library/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/protec-library/src/favicon.ico",
              "projects/protec-library/src/assets"
            ],
            "styles": [
              "./node_modules/primeicons/primeicons.css",
              "./node_modules/primeng/resources/themes/lara-light-blue/theme.css",
              "./node_modules/primeng/resources/primeng.min.css",
              "./node_modules/@angular/material/prebuilt-themes/purple-green.css",
              "projects/protec-library/src/assets/scss/style.scss",
              "projects/protec-library/src/assets/scss/theme.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/protec-library/src/environments/environment.ts",
                  "with": "projects/protec-library/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "protec-library:build:production"
            },
            "development": {
              "browserTarget": "protec-library:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "protec-library:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/protec-library/src/test.ts",
            "polyfills": "projects/protec-library/src/polyfills.ts",
            "tsConfig": "projects/protec-library/tsconfig.spec.json",
            "karmaConfig": "projects/protec-library/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/protec-library/src/favicon.ico",
              "projects/protec-library/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/purple-green.css",
              "projects/protec-library/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "protec"
}
