	</url-mapping>
	<url-mapping id="tenantrole">
		<pattern value="/module/tenantrole" />
		<view-id value="/module/tenantrole/tenantrole.oaf" />
	</url-mapping>

	<!-- Permissionsmanagement -->
	<url-mapping id="permissions">
		<pattern value="/module/permissions" />
		<view-id value="/module/permissions/permissions.oaf" />
	</url-mapping>
	<url-mapping id="permission">
		<pattern value="/module/permission" />
		<view-id value="/module/permission/permission.oaf" />
	</url-mapping>
	
	<url-mapping id="actions">
		<pattern value="/module/actions" />
		<view-id value="/module/permissions/actions.oaf" />
	</url-mapping>
	<url-mapping id="action">
		<pattern value="/module/action" />
		<view-id value="/module/permission/action.oaf" />
	</url-mapping>

	<url-mapping id="resources">
		<pattern value="/module/resources" />
		<view-id value="/module/permissions/resources.oaf" />
	</url-mapping>
	<url-mapping id="resource">
		<pattern value="/module/resource" />
		<view-id value="/module/permission/resource.oaf" />
